    <suki:SukiWindow.MenuItems>
        <MenuItem Header="Aurelio">
            <MenuItem Header="Hello" />
        </MenuItem>
    </suki:SukiWindow.MenuItems>
    <DockPanel>
        <suki:GlassCard
            HorizontalAlignment="Left"
            Margin="10,0,0,10"
            Padding="5"
            Width="218">
            <DockPanel>
                <TextBox
                    DockPanel.Dock="Top"
                    Margin="5,5,5,-5"
                    Text="{Binding SearchText, Mode=TwoWay}"
                    suki:TextBoxExtensions.AddDeleteButton="True">
                    <TextBox.InnerLeftContent>
                        <avalonia:MaterialIcon
                            Foreground="{DynamicResource TextColor}"
                            Height="15"
                            Kind="Search"
                            Margin="5,0"
                            VerticalAlignment="Center"
                            Width="15" />
                    </TextBox.InnerLeftContent>
                </TextBox>
                <ui:NavigationView
                    AlwaysShowHeader="False"
                    DockPanel.Dock="Bottom"
                    FontSize="14"
                    FontWeight="Bold"
                    FooterMenuItemsSource="{Binding FooterNavPages}"
                    HorizontalAlignment="Left"
                    IsBackButtonVisible="False"
                    IsBackEnabled="False"
                    IsPaneToggleButtonVisible="False"
                    IsSettingsVisible="False"
                    IsTabStop="False"
                    Margin="0,0,0,-10"
                    MenuItemsSource="{Binding FilteredNavPages}"
                    PaneDisplayMode="Left"
                    SelectionFollowsFocus="False"
                    x:Name="NavMenu">
                    <ui:NavigationView.Styles>
                        <Style Selector="ui|NavigationViewItem:leftnav Border#LayoutRoot">
                            <Setter Property="CornerRadius" Value="10" />
                        </Style>
                    </ui:NavigationView.Styles>
                    <ui:NavigationView.MenuItemTemplate>
                        <DataTemplate DataType="entity:NavPage">
                            <ui:NavigationViewItem
                                IsExpanded="{Binding IsExpanded, Mode=TwoWay}"
                                MenuItemsSource="{Binding SubPages}"
                                Tag="{Binding Id}">
                                <DockPanel>
                                    <PathIcon
                                        Data="{Binding Icon}"
                                        Height="16"
                                        Margin="0,0,10,0"
                                        Width="16" />
                                    <TextBlock Text="{Binding Header}" />
                                </DockPanel>
                            </ui:NavigationViewItem>
                        </DataTemplate>
                    </ui:NavigationView.MenuItemTemplate>
                </ui:NavigationView>
            </DockPanel>
        </suki:GlassCard>
        <ContentControl
            Content="{Binding ((entity:NavPage)SelectedItem).Content, ElementName=NavMenu}"
            Margin="0,-10,0,0"
            Name="Frame" />
    </DockPanel>
</suki:SukiWindow>