<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <Color x:Key="TextColor">#ffffff</Color>
            <Color x:Key="WindowBackground">#202020</Color>
            <Color x:Key="PageBackground">#242424</Color>
            <Color x:Key="TitleBarHoverColor">#272727</Color>
            <Color x:Key="ExpanderHoverColor">#2F2F2F</Color>
            <Color x:Key="GrayColor">#7e7e7e</Color>
            <Color x:Key="BorderBrush">#383838</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <Color x:Key="TextColor">#333333</Color>
            <Color x:Key="WindowBackground">#F2F4F7</Color>
            <Color x:Key="PageBackground">#FFFFFF</Color>
            <Color x:Key="TitleBarHoverColor">#EAEBEF</Color>
            <Color x:Key="ExpanderHoverColor">#F3F3F4</Color>
            <Color x:Key="GrayColor">#7e7e7e</Color>
            <Color x:Key="BorderBrush">#EAECF0</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>